{
  "name": "@hughescr/eslint-config-default",
  "version": "5.0.0",
  "description": "Default base config for eslint",
  "repository": {
    "type": "git",
    "url": "git://github.com/hughescr/eslint-config-default.git"
  },
  "license": "BSD-3-Clause",
  "author": "Craig Hughes <craig.npm@rungie.com>",
  "type": "module",
  "exports": "./index.mjs",
  "files": [
    "index.mjs"
  ],
  "sideEffects": false,
  "scripts": {
    "rules-checkup": "eslint-config-inspector",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postversion": "git commit -m \"Bump package version to $npm_package_version\" package.json; git flow release start $npm_package_version; git flow release finish -m $npm_package_version $npm_package_version; git checkout develop; git merge main"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.9.0",
    "eslint": "^10.0.2",
    "eslint-formatter-git-log": "^0.6.4",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-package-json": "^0.89.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^3.0.0",
    "typescript-eslint": "^8.56.1"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
